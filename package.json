{
    "name": "vioc-angular",
    "version": "0.0.0",
    "license": "TODO",
    "scripts": {
        "ng": "nx",
        "nx": "nx",
        "start": "ng serve --configuration=local",
        "start:es5": "ng serve --configuration=es5",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "pact-broker": "pact-broker",
        "pact:publish": "pact-broker publish ./pact/pacts",
        "affected:apps": "nx print-affected --type=app --select=projects",
        "affected:libs": "nx print-affected --type=lib --select=projects",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test --update-snapshot",
        "test:bottom-side": "nx test central-ui --ci --runInBand --code-coverage --reporters=jest-junit --reporters=default",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "format:launch-json": "nx format:write --files .vscode/launch.json",
        "update": "ng update @nx/workspace",
        "workspace-schematic": "nx workspace-schematic",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "gen-local-central": "more apps/central-ui/src/index.ejs | iwa-ejs --d apps/central-ui/.immutable/config-local.json > apps/central-ui/.immutable/index.html",
        "gen-local-bottom": "more apps/bottom-side-ui/src/index.ejs | iwa-ejs --d apps/bottom-side-ui/.immutable/config-local.json > apps/bottom-side-ui/src/index.html",
        "iwa-ejs": "iwa-ejs",
        "sort:nx-json": "sort-json --depth=2 nx.json && nx format:write --files=nx.json",
        "sort:tsconfig-json": "sort-json --depth=3 tsconfig.base.json && nx format:write --files=tsconfig.base.json",
        "sort:angular-json": "sort-json --depth=2 angular.json && nx format:write --files=angular.json",
        "postinstall": "copyfiles -f tools/git/hooks/* .git/hooks && node ./decorate-angular-cli.js",
        "merge-coverage": "lcov-result-merger coverage/**/lcov.info coverage/merged-lcov.info",
        "sonar-scanner": "sonar-scanner",
        "pre-commit": "nx format:check && nx affected:lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "16.0.2",
        "@angular/cdk": "16.0.1",
        "@angular/common": "16.0.2",
        "@angular/compiler": "16.0.2",
        "@angular/core": "16.0.2",
        "@angular/forms": "16.0.2",
        "@angular/localize": "16.0.2",
        "@angular/material": "16.0.1",
        "@angular/material-moment-adapter": "16.0.1",
        "@angular/platform-browser": "16.0.2",
        "@angular/platform-browser-dynamic": "16.0.2",
        "@angular/router": "16.0.2",
        "@ng-idle/core": "^12.0.4",
        "@nx/linter": "^17.3.0",
        "@stomp/rx-stomp": "^1.1.4",
        "core-js": "^3.30.2",
        "http-status-codes": "^1.4.0",
        "material-design-icons": "^3.0.1",
        "moment": "^2.27.0",
        "ngx-infinite-scroll": "16.0.0",
        "ngx-mat-select-search": "^7.0.2",
        "oidc-client": "^1.10.1",
        "oidc-client-ts": "^2.2.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rxjs": "~6.6.2",
        "tslib": "^2.5.2",
        "zone.js": "0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.1.4",
        "@angular-devkit/core": "16.0.2",
        "@angular-devkit/schematics": "16.0.2",
        "@angular-eslint/builder": "^16.1.0",
        "@angular-eslint/eslint-plugin": "16.0.2",
        "@angular-eslint/eslint-plugin-template": "16.0.2",
        "@angular-eslint/schematics": "^16.1.0",
        "@angular-eslint/template-parser": "16.0.2",
        "@angular/cli": "^16.1.4",
        "@angular/compiler-cli": "16.0.2",
        "@angular/language-service": "16.0.2",
        "@auth0/angular-jwt": "^5.0.1 ",
        "@immutablewebapps/ejs-cli": "0.0.2",
        "@nx/angular": "^16.5.1",
        "@nx/cypress": "16.7.4",
        "@nx/jest": "16.7.4",
        "@nx/js": "16.7.4",
        "@nx/storybook": "16.7.4",
        "@nx/web": "16.7.4",
        "@nx/workspace": "^16.5.1",
        "@pact-foundation/pact": "^11.0.2",
        "@schematics/angular": "16.0.2",
        "@storybook/addon-essentials": "7.6.10",
        "@storybook/addon-interactions": "^7.2.1",
        "@storybook/angular": "7.6.10",
        "@storybook/core-server": "7.6.10",
        "@storybook/jest": "~0.1.0",
        "@storybook/test-runner": "^0.13.0",
        "@storybook/testing-library": "~0.2.0",
        "@types/jest": "29.4.4",
        "@types/lodash": "^4.14.178",
        "@types/node": "20.2.3",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "codelyzer": "6.0.2",
        "copyfiles": "^2.3.0",
        "cypress": "^12.16.0",
        "eslint": "^8.39.0",
        "eslint-plugin-cypress": "^2.13.4",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "jest-junit": "^13.0.0",
        "jest-pact": "^0.10.3",
        "jest-preset-angular": "13.1.1",
        "lcov-result-merger": "^4.1.0",
        "nx": "16.1.4",
        "prettier": "2.6.2",
        "sonarqube-scanner": "^2.7.0",
        "sort-json": "^2.0.0",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "tslib": "^2.5.2",
        "typescript": "5.0.4"
    },
    "jest-junit": {
        "outputDirectory": "./junit",
        "uniqueOutputName": "true"
    }
}
